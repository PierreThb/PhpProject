{% extends "layout.html.twig" %}

{% block body %}
    <p>team page : new meeting</p>
    <hr>
    <div id="divnewmeeting">
        {{ form_start(form) }}
        <div id="">
            {{ form_row(form.date) }}
        </div>
        <div id="">
            {{ form_row(form.deadline) }}
        </div>
        <div id="">
            {{ form_row(form.room) }}
        </div>
        <div id="">
            {{ form_row(form.duration) }}
        </div>
        <div id="">
            {{ form_row(form.meetingLeader) }}
        </div>
        <div id="">
            {{ form_row(form.meetingSecretary) }}
        </div>
        <button type="submit">Save!</button>
        {{ form_end(form) }}
    </div>
{% endblock %}